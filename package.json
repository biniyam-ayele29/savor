{
  "name": "solito-blank",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/pg": "^8",
    "@types/react": "^18.2.21",
    "eslint": "^8.21.0",
    "pg": "^8.17.2",
    "turbo": "^2.4.4",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "native": "cd apps/expo && yarn start",
    "web": "cd apps/next && yarn next"
  },
  "packageManager": "yarn@4.7.0",
  "eslintConfig": {
    "extends": "next",
    "settings": {
      "next": {
        "rootDir": "apps/next/"
      }
    },
    "root": true
  },
  "prettier": {
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.93.2",
    "date-fns": "^4.1.0",
    "nativewind": "^4.0.1",
    "react-native-css-interop": "^0.2.1",
    "react-native-reanimated": "^4.2.1",
    "tailwindcss": "^3.4.0"
  }
}
